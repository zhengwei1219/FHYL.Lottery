    <script type="text/javascript">
        var getdataUrl = "/Buy/GetNextData";
        var n = 0, timer = null;
        var btns;
        var currExp = "";
        var lotteryType = '@ViewBag.lotteryType';
        $(function () {
            //countDown();
            currExp = "@ViewBag.expect";
            getdata(lotteryType);
            $('#dl-menu').dlmenu();
